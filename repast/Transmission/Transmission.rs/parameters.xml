<?xml version="1.0" encoding="UTF-8" ?>
<parameters>

	<parameter name="randomSeed" displayName="Default Random Seed" type="int" defaultValue="__NULL__" />

	<parameter name="nAgents" displayName="nAgent" type="int" defaultValue="50" />
	
	<parameter name="transmissionType" displayName="transmissionType" type="String" defaultValue="vertical" />
		
