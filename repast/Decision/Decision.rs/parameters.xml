<?xml version="1.0" encoding="UTF-8" ?>
<parameters>

	<parameter name="randomSeed" displayName="Default Random Seed" type="int" defaultValue="__NULL__" />
	
	<parameter name="xDim" displayName="xDim" type="int" 
						defaultValue="30" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		
		<parameter name="yDim" displayName="yDim" type="int" 
						defaultValue="30" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		
		<parameter name="numberOfAgents" displayName="numberOfAgents" type="int" 
						defaultValue="50" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		
		<parameter name="searchRadius" displayName="searchRadius" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		
		<parameter name="maxEnergy" displayName="maxEnergy" type="double" 
						defaultValue="100" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
		
		<parameter name="maxResource" displayName="maxResource" type="int" 
						defaultValue="100" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"							
		/>
		
		<parameter name="decisionType" displayName="decisionType" type="String" defaultValue="greedy" />
		
		<parameter name="memory" displayName="memory" type="boolean" defaultValue="false" />
		
		<parameter name="energyCost" displayName="energyCost" type="double" 
						defaultValue="25.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
		
		<parameter name="resourceGrowthRate" displayName="resourceGrowthRate" type="double" 
						defaultValue="25.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
		
			
		<parameter name="nSteps" displayName="nSteps" type="double" 
						defaultValue="1000" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>

</parameters>
